{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "/Html/index.html"
        },
        {
            "source": "/index.html",
            "destination": "/Html/index.html"
        }
    ],
    "headers": [
        {
            "source": "/Css/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/jS/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}